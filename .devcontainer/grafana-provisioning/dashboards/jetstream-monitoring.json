{
  "title": "JetStream Monitoring",
  "uid": "jetstream-monitoring",
  "version": 1,
    "panels": [
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "id": 1,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "jetstream_stream_messages_total",
            "refId": "A"
          }
        ],
        "title": "Total Messages in Streams",
        "type": "stat"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "id": 2,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "jetstream_consumer_pending_messages",
            "refId": "A"
          }
        ],
        "title": "Pending Messages by Consumer",
        "type": "graph"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "id": 3,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "rate(worker_messages_processed_total[5m])",
            "refId": "A"
          }
        ],
        "title": "Message Processing Rate",
        "type": "timeseries"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "id": 4,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "histogram_quantile(0.95, sum(rate(worker_processing_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "title": "Processing Duration (95th percentile)",
        "type": "gauge"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "id": 5,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "sum by (status) (worker_messages_processed_total)",
            "refId": "A"
          }
        ],
        "title": "Messages by Status",
        "type": "piechart"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "id": 6,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "circuit_breaker_state",
            "refId": "A"
          }
        ],
        "title": "Circuit Breaker State",
        "type": "stat"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${prometheus}"
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "id": 7,
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "expr": "worker_active_workers",
            "refId": "A"
          }
        ],
        "title": "Active Workers",
        "type": "gauge"
      }
    ],
    "refresh": "5s",
    "schemaVersion": 37,
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "Prometheus",
            "value": "Prometheus"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "name": "prometheus",
          "options": [],
          "query": "prometheus",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "datasource"
        }
      ]
    },
    "time": {
      "from": "now-15m",
      "to": "now"
    }
}