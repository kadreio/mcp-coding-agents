{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Level"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "DEBUG": {
                        "color": "blue",
                        "index": 0
                      },
                      "INFO": {
                        "color": "green",
                        "index": 1
                      },
                      "WARN": {
                        "color": "yellow",
                        "index": 2
                      },
                      "ERROR": {
                        "color": "red",
                        "index": 3
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Time"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 180
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Message"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "auto",
                  "wrapText": true
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 20,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": ["sum"],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Time"
          }
        ]
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "expr": "{service=~\"${service}\"} |~ \"${filter:regex}\" | json msg=\"msg\", level=\"level\", timestamp=\"timestamp\", service=\"app_service\", error=\"error\", request_id=\"request_id\", method=\"method\", path=\"path\", status=\"status\", duration_ms=\"duration_ms\" | line_format \"{{if .msg}}{{.msg}}{{else}}{{__line__}}{{end}}\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Unified Logs",
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "labels"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "id": true,
              "tsNs": true,
              "job": true,
              "filename": true,
              "app_service": false
            },
            "indexByName": {
              "Time": 0,
              "level": 1,
              "service": 2,
              "container": 3,
              "Line": 4,
              "error": 5,
              "request_id": 6,
              "method": 7,
              "path": 8,
              "status": 9,
              "duration_ms": 10
            },
            "renameByName": {
              "Line": "Message",
              "level": "Level",
              "service": "Service",
              "container": "Container",
              "app_service": "App",
              "error": "Error",
              "request_id": "Request ID",
              "method": "Method",
              "path": "Path",
              "status": "Status",
              "duration_ms": "Duration (ms)"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 38,
  "tags": ["logs"],
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
