        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "id": true,
              "job": true,
              "tsNs": true,
              "labels": true
            },
            "indexByName": {
              "Time": 0,
              "container": 1,
              "service": 2,
              "stream": 3,
              "Line": 4
            },
            "renameByName": {
              "Line": "Log Message",
              "container": "Container",
              "service": "Service",
              "stream": "Stream"
            }
          }
        }
      ],
      "type": "table"
