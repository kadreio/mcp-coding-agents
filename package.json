{
  "name": "@kadreio/mcp-coding-agents",
  "version": "1.2.0",
  "main": "dist/index.js",
  "bin": {
    "mcp-coding-agents": "./dist/cli-unified.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:watch": "tsc --watch",
    "mcp": "node dist/cli-unified.js",
    "mcp:dev": "ts-node src/cli-unified.ts",
    "mcp:stdio": "node dist/cli-unified.js stdio",
    "mcp:stdio:dev": "ts-node src/cli-unified.ts stdio",
    "mcp:http": "node dist/cli-unified.js http",
    "mcp:http:dev": "ts-node src/cli-unified.ts http",
    "mcp:legacy": "node dist/mcp-server-http.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "release:beta": "./scripts/release.sh prerelease beta"
  },
  "author": "kardio",
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "ai",
    "server"
  ],
  "description": "MCP server with multiple AI coding agents for enhanced development workflows",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadreio/mcp-coding-agents.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.58",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
